{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["../App/frontend/src/*"]
    }
  },
  "include": ["../App/frontend/next-env.d.ts", "../App/frontend/**/*.ts", "../App/frontend/**/*.tsx", "../App/frontend/.next/types/**/*.ts"],
  "exclude": ["../App/frontend/node_modules"]
}
